<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stivenduque.clinicapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity
            android:name=".Activity.MenuProfile"
            android:label="@string/title_activity_menu_profile" />
        <activity android:name=".Activity.Login" />
        <activity
            android:name=".Activity.SplashScreen"
            android:theme="@style/Theme.Transparent">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.RegisterTabs"
            android:label="@string/title_activity_register_tabs"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".Activity.MyAccount"
            android:label="@string/title_activity_my_accunt"
            android:theme="@style/AppTheme2" />
        <activity android:name=".Activity.ChatActivity"></activity>
    </application>

</manifest>